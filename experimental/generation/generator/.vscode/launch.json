            "cwd": "${workspaceFolder}/packages/library",
            "args": [
                "--timeout",
                "999999",
                "--colors",
                "-g",
                ".*"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "outFiles": [
                "lib/**"
            ]
        },
